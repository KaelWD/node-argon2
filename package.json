    "@types/node": "24.9.1",
    "@types/phc__format": "^1.0.1",
    "tsdown": "^0.15.11",
    "typescript": "5.9.3",
    "vitest": "^4.0.4"
  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=24.7.0"
